# Environment Configuration
NODE_ENV=test

# API Configuration
VITE_API_BASE_URL=http://localhost:4001
VITE_API_VERSION=v1
VITE_API_TIMEOUT=5000

# Application Settings
VITE_APP_TITLE=Enrollment System - Test
VITE_AUTH_STORAGE_PREFIX=enrollment_system_test

# Feature Flags
VITE_ENABLE_LOGS=true
VITE_ENABLE_MOCK_API=true
VITE_ENABLE_MFA=false
VITE_ENABLE_ERROR_MONITORING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Document Upload Settings
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.jpg,.jpeg,.png

# Security Settings
VITE_SESSION_TIMEOUT=300000